<%= form_with(url: sessions_path, scope: :session, local: true) do |f| %>
    <%= f.label :email %> 
    <%= f.email_field :email %> <br> 

    <%= f.label :password %> 
    <%= f.password_field :password %> <br>  

    <%= f.submit "Log in" %> 

    <P> New user? <%= link_to "Sign up", new_user_path %></p> 
<% end %> 